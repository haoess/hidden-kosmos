Die <hi rendition="#aq"><choice><orig>Kaliphen</orig><reg resp="#cab">Kalifen</reg></choice></hi> <choice><abbr>ud</abbr><expan resp="#SB">und</expan></choice> <hi rendition="#aq"><choice><abbr>mongol</abbr><expan resp="#SB"><choice><orig>mongoliſchen</orig><reg resp="#cab">mongolischen</reg></choice></expan></choice></hi> <choice><orig>Furſten</orig><reg resp="#SB"><choice><orig>Fürſten</orig><reg resp="#cab">Fürsten</reg></choice></reg></choice><lb/>
