<del rendition="#s">Oft in</del> nur 1 <choice><orig>½°</orig><reg resp="#cab">1/2°</reg></choice> <choice><abbr>sd</abbr><expan resp="#TK">sind</expan></choice> <choice><abbr>d</abbr><expan resp="#TK">die</expan></choice> <hi rendition="#aq">maxima</hi> <choice><abbr>verschied</abbr><expan resp="#TK">verschieden</expan></choice>.<lb/>
